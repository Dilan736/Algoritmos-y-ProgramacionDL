Algoritmo descuento_al_publico
	escribir"ingresar el precio de venta al publico:"
	leer precio_al_publico
	Escribir "ingresar el precio final pagado:"
	leer precio_final_pagado
	porcentaje_descuento=((precio_al_publico-precio_final_pagado)/precio_al_publico)*100
	Escribir "el porcentaje de descuento aplicado es:",porcentaje_descuento,"%"
FinAlgoritmo
