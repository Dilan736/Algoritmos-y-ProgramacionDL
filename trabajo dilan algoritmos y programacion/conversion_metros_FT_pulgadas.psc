Algoritmo conversion_metros_FT_pulgadas
	Escribir"igresar cantidad en metros:"
	Leer metros
	pulgadas_total=metros*39.27
	FT=trunc(pulgadas_total/12)
	pulgadas_restantes=pulgadas_total-(FT*12)
	Escribir "cantidad en FT es:",FT
	Escribir"cantidad en pulgadas es:",pulgadas_restantes
	
	
	
	
	
FinAlgoritmo
